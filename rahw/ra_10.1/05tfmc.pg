ANS($q1->cmp());
ANS(checkbox_cmp($q2->correct_ans()));
ANS(checkbox_cmp($q3->correct_ans()));
ANS($q4->cmp());
ANS($q5->cmp());
ANS($q6->cmp());
ANS($q7->cmp());
ANS(checkbox_cmp($q8->correct_ans()));
ANS($q9->cmp());

