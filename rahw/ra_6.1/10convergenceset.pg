"\((0,1]\)",
"\((0,1)\)",
"\($LB 0, 1 $RB\)",
"\($LB 1 $RB\)",
"\($LB 0 $RB\)",
"\( \emptyset \)",
],
"\([0,1]\)",
);
Context()->texStrings;
BEGIN_TEXT
\{ $q1->buttons \}

